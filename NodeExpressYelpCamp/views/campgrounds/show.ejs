<% include ../partials/header %>

<div class="container">
  <h1><%= campground.name%></h1>
  <img src="<%=campground.image%>" alt="">
  <p><%=campground.description%></p>
  <h3>Comments</h3>
  <%campground.comments.forEach(function(comment){%>
    <p><%= comment.text%> -<b><%= comment.author%></b> </p>
  <%})%>
</div>
